/* components/markDayPannel/markDayPannel.wxss */
.wrapper{
    margin:30rpx;
    padding:30rpx;
    border-radius: 20rpx;
    background-color: #fff;
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-weight: 300;
}
.icon{
    margin-right:30rpx;
    width:40rpx;
    height:40rpx;
}
.nameBox{
    font-size:30rpx;
    font-weight: 300;
    display: flex;
    justify-content: flex-start;
    align-items: center;

}
.name{
    max-width:400rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.time{
    color:#aaa;
    display: flex;
    justify-content: center;
    align-items: center;
}
.add{
    width:40rpx;
    height:40rpx;
}
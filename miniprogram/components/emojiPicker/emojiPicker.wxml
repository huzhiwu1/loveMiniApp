<view class='wrapper'>
    <view class='selectedEmoji'>
        <text class='title'>此刻心情</text>
         <image class='img'bindtap='showEmoji' src='/images/smile.png' />
    </view>
    <view animation='{{animationData}}' class='emojiBox'>
        <image class='img' bindtap='selectEmoji' data-url='/images/smile.png' src='/images/smile.png' />
        <image class='img' bindtap='selectEmoji' src='/images/moving.png' data-url='/images/moving.png' />
        <image class='img' bindtap='selectEmoji' src='/images/cry.png' data-url='/images/cry.png'/>
    </view>
</view>